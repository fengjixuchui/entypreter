#!/bin/bash

cd ~
if [[ -d ~/proton ]]
then
{
cd ~/proton
} &> /dev/null
cd
cd proton/script
chmod +x pscript
./pscript $1 $2 $3 $4 $5 $6 $7 $8 $9
else
{
ASESR="$( curl -s checkip.dyndns.org | sed -e 's/.*Current IP Address: //' -e 's/<.*$//' )"
} &> /dev/null
if [[ "$ASESR" != "" ]]
then 
cd ~
{
git clone https://github.com/entynetproject/proton.git
cd ~/proton
} &> /dev/null
cd
cd proton/script
chmod +x pscript
./pscript $1 $2 $3 $4 $5 $6 $7 $8 $9
else
cd ~
sleep 1
exit
fi
fi
