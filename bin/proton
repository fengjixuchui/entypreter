printf '\033]2;Proton Framework\a'

if [[ -d ~/proton ]]
then
{
cd ~/proton
mkdir etc/.disabled
mv modules/implant/util/upload_file.py etc/.disabled
mv etc/modules/implant/util/upload_file.py modules/implant/util
cp etc/.disabled/upload_file.py etc/modules/implant/util
rm -rf etc/.disabled
mkdir etc/.disabled
mv modules/implant/util/download_file.py etc/.disabled
mv etc/modules/implant/util/download_file.py modules/implant/util
cp etc/.disabled/download_file.py etc/modules/implant/util
rm -rf etc/.disabled
mkdir etc/.disabled
mv core/commands/pyexec.py etc/.disabled
mv etc/core/commands/pyexec.py core/commands
cp etc/.disabled/pyexec.py etc/core/commands
rm -rf etc/.disabled
} &> /dev/null
chmod +x proton
./proton
{
if [[ -f ~/proton/.updated ]]
then
rm ~/proton/.updated
else
cd ~/proton
mkdir etc/.disabled
mv modules/implant/util/upload_file.py etc/.disabled
mv etc/modules/implant/util/upload_file.py modules/implant/util
cp etc/.disabled/upload_file.py etc/modules/implant/util
rm -rf etc/.disabled
mkdir etc/.disabled
mv modules/implant/util/download_file.py etc/.disabled
mv etc/modules/implant/util/download_file.py modules/implant/util
cp etc/.disabled/download_file.py etc/modules/implant/util
rm -rf etc/.disabled
mkdir etc/.disabled
mv core/commands/pyexec.py etc/.disabled
mv etc/core/commands/pyexec.py core/commands
cp etc/.disabled/pyexec.py etc/core/commands
rm -rf etc/.disabled
fi
} &> /dev/null
else
{
ASESR="$(ping -c 1 -q www.google.com >&/dev/null; echo $?)"
} &> /dev/null
if [[ "$ASESR" = 0 ]]
then 
cd ~
{
git clone https://github.com/entynetproject/proton.git
cd $OLDPWD
cd ~/proton
mkdir etc/.disabled
mv modules/implant/util/upload_file.py etc/.disabled
mv etc/modules/implant/util/upload_file.py modules/implant/util
cp etc/.disabled/upload_file.py etc/modules/implant/util
rm -rf etc/.disabled
mkdir etc/.disabled
mv modules/implant/util/download_file.py etc/.disabled
mv etc/modules/implant/util/download_file.py modules/implant/util
cp etc/.disabled/download_file.py etc/modules/implant/util
rm -rf etc/.disabled
mkdir etc/.disabled
mv core/commands/pyexec.py etc/.disabled
mv etc/core/commands/pyexec.py core/commands
cp etc/.disabled/pyexec.py etc/core/commands
rm -rf etc/.disabled
} &> /dev/null
chmod +x proton
./proton
{
if [[ -f ~/proton/.updated ]]
then
rm ~/proton/.updated
else
cd ~/proton
mkdir etc/.disabled
mv modules/implant/util/upload_file.py etc/.disabled
mv etc/modules/implant/util/upload_file.py modules/implant/util
cp etc/.disabled/upload_file.py etc/modules/implant/util
rm -rf etc/.disabled
mkdir etc/.disabled
mv modules/implant/util/download_file.py etc/.disabled
mv etc/modules/implant/util/download_file.py modules/implant/util
cp etc/.disabled/download_file.py etc/modules/implant/util
rm -rf etc/.disabled
mkdir etc/.disabled
mv core/commands/pyexec.py etc/.disabled
mv etc/core/commands/pyexec.py core/commands
cp etc/.disabled/pyexec.py etc/core/commands
rm -rf etc/.disabled
fi
} &> /dev/null
else
cd ~
sleep 1
exit
fi
fi
